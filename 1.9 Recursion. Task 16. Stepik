#include <iostream>
#include <vector>
using namespace std;

vector <int> v(11);

bool CanPlaceNextQuin (int i, int value){
  //int k = i - 1;
  for (int k = i - 1; k > 0; k--){
    if (abs(i - k) != abs(v[i] - v[k]));
    else return False;
  }
return True;
}

int place (int x, int y, int z){
	if (x == z) return 1;
	else return place(++x, ++y, z);
}


int main (){
  int n, pntr = 0, point, val;
  int init = 1, output = 1;
  cin >> n >> point >> val;
  //vector <int> v(n);
  
  //inite v[i]
  init = 1;
  if (init == 1) {
    for (int i = 0; i < n; i++){
      v[i] = 0;
    }
  }
  v[0] = 2;
  
  cout << n << endl;
  cout << "Funk " << place(1, v[0], n) << endl;
  cout << "CanPlaceNextQuin: " << CanPlaceNextQuin(point, val) << endl;
  //output of v[i]
  output = 1;
  if (output == 1) {
    for (int i = 0; i < n; i++){
      cout << v[i] << " ";
    }
  }


return 0;
}
