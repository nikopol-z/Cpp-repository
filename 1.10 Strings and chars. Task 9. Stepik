#include <iostream>
#include <string>

using namespace std;

int main(){
	int j = 1, n = 0, i = 0;
	int dots = 0;
	//char c;
	vector <int> p(5);
	string s;
	getline(cin, s);
	n = s.size();
	p[0] = -1;
	p[4] = n;
	if (n <= 15 && n >= 7) {
		while ((int) s.find('.', i) != -1 || dots > 3){
			dots++;
			p[j] = i;
		}
		if (dots != 3) cout << "no";
		else {
			if (NumIsCorrect(s, i)) cout << "yes";
			else cout << "no";
		}
	} else cout << "no";

	
	
	
	return 0;
}
